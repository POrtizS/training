PROCEDURE "training.procedures::TEST"(
IN P_ID INTEGER
)
LANGUAGE SQLSCRIPT
SQL SECURITY DEFINER
--DEFAULT SCHEMA "8D6D55BDCB64464A8C9B940DCE924F77"
AS
BEGIN
declare i Integer;
 SELECT COUNT(*) INTO i FROM "TRAINING_TESTE_BUFFER" WHERE ID = :P_ID;
 IF(i = 0) THEN 
  INSERT INTO "TRAINING_TESTE_BUFFER" VALUES(:P_ID); 
  END IF;
END